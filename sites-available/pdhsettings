server {
	listen   80; 

    server_name  www.linandg.com linandg.com *.linandg.com;
 
 	location / {
	    proxy_pass  http://blog.thepaydayhound.com;
	    proxy_redirect off;
	    proxy_buffering off;

    }
 
    location /tools {
    	proxy_pass  http://dev.thepaydayhound.com/;
    	proxy_redirect off;
     	proxy_buffering off;
    }
}
